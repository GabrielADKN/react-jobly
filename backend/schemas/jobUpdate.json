{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "minLength": 1,
            "maxLength": 30,
            "title": "The title Schema",
            "examples": [
                "New"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                100
            ]
        },
        "equity": {
            "type": "string",
            "default": "",
            "minLength": 1,
      "maxLength": 30,
            "title": "The equity Schema",
            "examples": [
                "0.01"
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "minLength": 1,
      "maxLength": 30,
            "title": "The companyHandle Schema",
            "examples": [
                "c1"
            ]
        }
    },
    "examples": [
        {
            "title": "New",
            "salary": 100,
            "equity": "0.01",
            "companyHandle": "c1"
        }
    ]
}